<h3>登録情報</h3>
<table>
  <thead>
    <tr>
      <th>ルーム画像</th>
      <th>ルーム名</th>
      <th>ルーム紹介</th>
      <th>料金</th>
      <th>作成日</th>
    </tr>
  </thead>

  <tbody>
    <% @rooms.each do |room| %>
      <tr>
        <td><%= room.room_name %></td>
        <td><%= image_tag room.roomphoto.url, :size => '50x50' %></td>
        <td><%= room.room_intr %></td>
        <td><%= room.room_price %></td>
        <td><%= room.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>
